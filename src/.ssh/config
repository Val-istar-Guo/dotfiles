Include ~/.dotfiles/ssh/*

HostKeyAlgorithms +ssh-rsa

# Host github.com
#     Hostname github.com
#     PreferredAuthentications publickey
#     User git
#     Identityfile ~/.ssh/license/github/id_rsa

# Host gist.github.com
#     Hostname gist.github.com
#     User git
#     PreferredAuthentications publickey
